
package creating.objects;

public class CreatingObjects {

    public static void main(String[] args) {
        
        person p1=new person();
        p1.name="alvi";
        p1.id=10;
        //System.out.println(p1.name+"   "+p1.id);
        p1.eat();
    }
    
}
class person{
    String name;
    int id;
    void eat(){
        System.out.println(name+" is eating now");
        
    }
}
