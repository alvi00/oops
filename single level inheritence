
package javaapplication47;

public class JavaApplication47 {

    
    public static void main(String[] args) {
        fish f1=new fish();
        f1.mouth();
        
    }
    
}
class animal{
    int leg;
    void mouth(){
        System.out.println("Have mouth");
    }
}
class fish extends animal{
    int hand;
    void nose(){
        System.out.println("Have nose");
    }
}
